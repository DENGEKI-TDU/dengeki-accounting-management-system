generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model account {
  id            Int @id @default(autoincrement())
  date          DateTime
  type          String
  typeAlphabet  String
  subtype       String
  fixture       String
  income        Int
  outcome       Int
}